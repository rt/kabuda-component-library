{
  "name": "kabuda-components-library",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9"
  ],
  "scripts": {
    "start": "npm run clean && babel-node tools/start",
    "debug": "npm start -- --inspect",
    "clean": "babel-node tools/clean",
    "copy": "babel-node tools/copy",
    "bundle": "babel-node tools/bundle",
    "build": "npm run clean && npm run copy && npm run bundle",
    "render-static": "npm run build && babel-node tools/render",
    "test": "jest",
    "testOld": " mocha --require @babel/register 'src/utils/test_setup.js' 'src/**/*.spec.js'",
    "test:watch": "npm run test -- --watch",
    "test:watch:all": "npm run test -- --watchAll",
    "coverOld": "nyc --reporter=html npm run test && nyc report",
    "cover": "npm test -- --coverage",
    "automation": "mocha --require @babel/register --colors 'automation/test_setup.js' 'automation/scenarios/**/*.spec.js'",
    "automation:debug": "AUTOMATION_BASE_URL=http://localhost:3000 npm run automation -- --inspect",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint src/component-library src/components src/libs src/routes src/utils",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:css": "stylelint src/**/*.{css,less,scss,sss}"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "accounting": "0.4.1",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.4",
    "draft-js": "0.10.1",
    "deepmerge": "2.0.1",
    "express": "^4.16.3",
    "google-maps-react": "^2.0.2",
    "history": "^4.7.2",
    "isomorphic-style-loader": "^4.0.0",
    "kabuda-liquid": "^0.4.12",
    "lorem-ipsum": "1.0.3",
    "moment": "^2.23.0",
    "node-fetch": "^2.1.2",
    "pretty-error": "^2.1.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.10.1",
    "react": "^16.8.2",
    "react-ace": "6.0.0",
    "react-big-calendar": "0.15.0",
    "react-day-picker": "6.1.0",
    "react-dom": "^16.8.2",
    "react-icons": "2.2.7",
    "react-markdown": "2.5.0",
    "react-transition-group": "2.2.1",
    "serialize-javascript": "^1.5.0",
    "source-map-support": "^0.5.6",
    "universal-router": "^6.0.0",
    "uuid": "3.0.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.8.3",
    "@babel/plugin-transform-react-inline-elements": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "autoprefixer": "^8.6.3",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "browser-sync": "^2.24.4",
    "chokidar": "^2.1.1",
    "chromedriver": "2.38.2",
    "css-loader": "^0.28.11",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-node": "^0.3.3",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-css-modules": "^2.7.5",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.18.0",
    "file-loader": "^1.1.11",
    "front-matter": "^2.3.0",
    "glob": "^7.1.2",
    "http-proxy-middleware": "^0.19.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "kabuda-nodejs-tools": "^0.0.10",
    "markdown-it": "^8.4.1",
    "mkdirp": "^0.5.1",
    "null-loader": "^0.1.1",
    "opn-cli": "^3.1.0",
    "pixrem": "^4.0.1",
    "pleeease-filters": "^4.0.0",
    "postcss": "^6.0.22",
    "postcss-calc": "^6.0.1",
    "postcss-color-function": "^4.0.1",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^7.0.0",
    "postcss-custom-selectors": "^4.0.1",
    "postcss-flexbugs-fixes": "^3.3.1",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.5",
    "postcss-media-minmax": "^3.0.0",
    "postcss-nested": "^3.0.0",
    "postcss-nesting": "^6.0.0",
    "postcss-pseudoelements": "^5.0.0",
    "postcss-selector-matches": "^3.0.1",
    "postcss-selector-not": "^3.0.1",
    "raw-loader": "^0.5.1",
    "react-deep-force-update": "^2.1.1",
    "react-dev-utils": "^5.0.1",
    "react-error-overlay": "^4.0.0",
    "react-test-renderer": "^16.8.2",
    "rimraf": "^2.6.2",
    "selenium-webdriver": "^3.0.0-beta-3",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-order": "^0.8.1",
    "svg-url-loader": "^2.3.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.29.4",
    "webpack-assets-manifest": "^3.0.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-middleware": "^3.5.2",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./src/utils/test_setup.js"
    ],
    "coverageReporters": [
      "html"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy",
      "isomorphic-style-loader/lib/withStyles": "<rootDir>/__mocks__/withStyles.js"
    }
  }
}
